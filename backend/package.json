{
  "name": "@migum/server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint .",
    "tsc:check": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start:dev": "ts-node-dev --inspect=0.0.0.0:9229 --poll --respawn src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:generate": "npm run typeorm -- migration:generate -n ",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:create": "npm run typeorm -- migration:create -n"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/amqplib": "^0.5.17",
    "amqplib": "^0.7.1",
    "arrow-express": "0.2.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^7.0.4",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "npm": "^7.6.2",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.21",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
